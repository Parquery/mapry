{
  "name": "Some_graph",
  "description": "defines some object graph.",
  "cpp": {
    "namespace": "some::graph",
    "path_as": "boost::filesystem::path",
    "optional_as": "boost::optional",
    "datetime_library": "ctime"
  },
  "go": {
    "package": "somegraph"
  },
  "py": {
    "module_name": "some.graph",
    "path_as": "pathlib.Path",
    "timezone_as": "str"
  },
  "properties": {
    "some_path": {
      "type": "path",
      "description": "defines some path.",
      "pattern": "^/[a-zA-Z]+-[0-9]+$"
    },
    "unconstrained_path": {
      "type": "path",
      "description": "defines an unconstrained path."
    }
  }
}
